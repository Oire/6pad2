"^S", IDM_SAVE_AS, SHIFT
"^O", IDM_OPEN_NI, SHIFT
"^a", IDM_SELECTALL
"^g", IDM_GOTOLINE
"^f", IDM_FIND
"^h", IDM_REPLACE
"^z", IDM_UNDO
"^Z", IDM_REDO, SHIFT
//VK_HOME, IDM_REALHOME, ALT, VIRTKEY
//VK_F4, IDM_SWITCHCURSOR, VIRTKEY
